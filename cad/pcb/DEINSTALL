#! /bin/sh
#
# $NetBSD$
#

case "$2" in
	DEINSTALL) cat <<EOF

=============================================================
Note that the local library configuration file
(@pkg_sysconfdir@/pcb/local.inc) is not removed by the
deinstallation process.  You should remove this by hand, if
you no longer need it.
=============================================================

EOF
	;;
esac
