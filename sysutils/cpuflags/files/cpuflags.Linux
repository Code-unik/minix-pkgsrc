#!/bin/sh
# $NetBSD$

hw_model=`uname -m`

case $hw_model in
    # i386
    i386*)		FLAGS='-march=i386'		;;
    i486*)		FLAGS='-march=i486'		;;
    i586*)		FLAGS='-march=pentium'		;;
    i686*)		FLAGS='-march=pentiumpro'	;;
    #
    *)			echo "Unknown hw.model '$hw_model'" >&2
esac

echo $FLAGS

exit 0
