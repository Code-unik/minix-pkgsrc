# ENABLE floating point emulation for MINIX

FPBIT=fp-bit.c
DPBIT=dp-bit.c

fp-bit.c: $(srcdir)/config/fp-bit.c
	rm -f fp-bit.c
	echo '#define FLOAT' > fp-bit.c
	cat $(srcdir)/config/fp-bit.c >> fp-bit.c

dp-bit.c: $(srcdir)/config/fp-bit.c
	rm -f dp-bit.c
	echo '' > dp-bit.c
	cat $(srcdir)/config/fp-bit.c >> dp-bit.c
