# $NetBSD: DEINSTALL,v 1.1 2003/11/03 00:56:25 heinz Exp $

case ${STAGE} in
DEINSTALL)
	${CAT} << EOF
===========================================================================
Note:

  Removing Assemblies from the Global Assembly Cache.

===========================================================================
EOF

assemblies=`${LS} @PREFIX@/lib/mono/gac`

for a in $assemblies; do
	@PREFIX@/bin/gacutil -u $a
done
	;;
esac
