CROSSINC=${PKG_PREFIX}/cross/i386-netbsdpe/include

case ${STAGE} in
POST-DEINSTALL)
	${RM} ${CROSSINC}/ctype.h
	${RM} ${CROSSINC}/stdio.h
	${RM} ${CROSSINC}/stdlib.h
	${RM} ${CROSSINC}/time.h
	${RM} ${CROSSINC}/unistd.h
#	${RM} ${CROSSINC}/i386/ansi.h
#	${RMDIR} ${CROSSINC}/i386
#	${RM} ${CROSSINC}/machine
	${RM} ${CROSSINC}/sys/param.h
	${RMDIR} ${CROSSINC}/sys
	;;
esac
