#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.2 2003/01/20 09:47:14 wiz Exp $
#

VARBASE=@VARBASE@

case "${STAGE}" in
	POST-DEINSTALL) cat << EOF
=============================================================
The score files were not wiped out by this deletion process.
If you don't want them around, please remove the directory:
  ${VARBASE}/games/xmris
and its contents.
=============================================================
EOF
	;;
esac
