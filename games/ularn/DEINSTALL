#!/bin/sh
#
# $NetBSD: DEINSTALL,v 1.1.1.1 2002/01/21 11:56:16 pooka Exp $
#

case $2 in
        DEINSTALL) cat << EOF

	=============================================================
	The score was not wiped out by this deletion process. If you
	don't want it around, please remove
	${PKG_PREFIX}/share/Ularn.
	=============================================================

EOF
	;;
esac
