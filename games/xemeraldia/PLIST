bin/xemeraldia
lib/X11/app-defaults/XEmeraldia
@exec ${MKDIR} -p %D/${GAMES_DIR}/xemeraldia
@exec touch -f %D/${GAMES_DIR}/xemeraldia/xemeraldia.scores
