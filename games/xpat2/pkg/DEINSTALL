#!/bin/sh
#
# $NetBSD$

PKGNAME=$1
STAGE=$2

case ${STAGE} in
DEINSTALL)
	;;
POST-DEINSTALL)
	cat << EOF
===========================================================================
If you won't be using ${PKGNAME} any longer, you may want to remove the
following files:

	/var/games/xpat.log
===========================================================================
EOF
	;;
*)
	echo "Unexpected argument: ${STAGE}"
	exit 1
	;;
esac
exit 0
