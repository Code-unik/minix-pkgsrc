#! /bin/sh
#
# $NetBSD: DEINSTALL,v 1.1 2001/10/31 22:55:03 zuntum Exp $

case ${STAGE} in
POST-DEINSTALL)
	for file in au coda-setup-ports codaconfedit; do
		${RM} -f ${PKG_PREFIX}/sbin/${file}
		if [ -f ${PKG_PREFIX}/sbin/venus-${file} ]; then
			${LN} -sf venus-${file} ${PKG_PREFIX}/sbin/${file}
		fi
	done
	;;
esac
