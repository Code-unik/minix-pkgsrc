Treat DragonFly like FreeBSD.

--- mozilla/js/src/Makefile.in.orig	2012-02-16 14:09:03.000000000 +0000
+++ mozilla/js/src/Makefile.in
@@ -809,6 +809,9 @@ endif # WINNT
 ifeq ($(OS_ARCH),FreeBSD)
 EXTRA_LIBS	+= -pthread
 endif
+ifeq ($(OS_ARCH),DragonFly)
+EXTRA_LIBS	+= -pthread
+endif
 ifeq ($(OS_ARCH),Linux)
 EXTRA_LIBS	+= -ldl
 endif
